{
  "$schema": "./node_modules/nx/schemas/nx-schema.json", 
  "targetDefaults": {
    "test": {
      "dependsOn": ["^test"],
      "cache": true
    }
  },
  "defaultBase": "main",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test"
      }
    }
  ]
}
